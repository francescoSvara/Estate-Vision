  "context7MCP": {
    "purpose": "Retrieve latest stable documentation from external library sources",
    "note": "Context7 fetches current official docs that may differ from AI training data. Always use for external dependencies to ensure accuracy.",
    "whenToUse": [
      "Before implementing features with external dependencies (MapLibre GL JS, PMTiles, Vite, etc.)",
      "When API signatures or library behavior may have changed since training data",
      "To verify current best practices for third-party integrations",
      "When troubleshooting external library usage",
      "To check if current library versions support specific features",
      "To find breaking changes or migration guides"
    ],
    "workflow": {
      "step_1": {
        "tool": "mcp_upstash_conte_resolve-library-id",
        "input": "library_name (e.g., 'maplibre-gl', 'pmtiles')",
        "output": "context7_id (e.g., '/maplibre/maplibre-gl-js')"
      },
      "step_2": {
        "tool": "mcp_upstash_conte_get-library-docs",
        "input": "context7_id + optional topic",
        "output": "current documentation and code examples"
      }
    },
    "doNotUseFor": [
      "Project-specific code (use semantic_search, read_file, grep_search)",
      "Internal APIs or services",
      "Custom components in src/components/"
    ]
  },
